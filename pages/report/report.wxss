.container-report {
  width: 100vw;
  position: relative;
}

.user-info {
  display: flex;
  width: 710rpx;
  margin: 0 auto 30rpx auto;
  height: 300rpx;
}

.class-info {
  display: flex;
  width: 710rpx;
  margin: 0 auto 30rpx auto;
}

.exercise-list {
  display: flex;
  width: 710rpx;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 0 auto;
}

.action-sets {
  display: flex;
  flex-direction: column;
  width: 30vw;
  background: #efefef;
  border-radius: 20rpx;
  margin-bottom: 10rpx;
  color: #333333;
  padding-bottom: 10rpx;
}

.movement-list {

}

.action-name {
  font-weight: bold;
  font-size: 28rpx;
  margin: 10rpx 0 0 20rpx;
}

.plan-date {
  margin: 20rpx auto;
  font-size: 32rpx;
  text-align: center;
}


.title {
  margin: 0;
  padding: 16px;
  color: rgba(69, 90, 100, 0.6);
  font-weight: normal;
  font-size: 14px;
  line-height: 16px;
}

.empty-tip {
  display: flex;
  justify-content: center;
}
